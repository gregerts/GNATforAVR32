project Test is

  for Main use ("main");

  for Source_Dirs use ("./", "../common");

  package Builder is
     for Default_Switches ("Ada")
       use ("--GCC=/usr/local/bin/avr32-gcc",
            "--RTS=../../build/");
  end Builder;

  package Compiler is
     for Default_Switches ("Ada")
       use ("-gnatn",
            "-gnatw.X",
            "-nostartfiles",
            "-nostdinc",
            "-O2");
  end Compiler;

  package Linker is
     for Default_Switches ("Ada")
       use ("--GCC=/usr/local/bin/avr32-gcc",
            "--LINK=/usr/local/bin/avr32-gcc",
            "-nostartfiles",
            "-D0",
            "-d1k",
            "ada_crt0.o",
	    "-T../common/link.x",
            "-march=avr32elf",
            "-mpart=uc3a0512");
  end Linker;

end Test;
