project Test is

  for Main use ("main");

  for Source_Dirs use ("./", "../common");

  package Builder is
     for Default_Switches ("Ada")
       use ("--GCC=avr32-gcc",
            "--RTS=../../build/");
  end Builder;

  package Compiler is
     for Default_Switches ("Ada")
       use ("-gnatan",
            "-g",
            "-O2");
  end Compiler;

  package Linker is
     for Default_Switches ("Ada")
       use ("--GCC=avr32-gcc",
            "--LINK=avr32-gcc",
            "-nostartfiles",
	    "ada_crt0.o",
	    "-march=avr32elf",
            "-mpart=uc3a0512es",
	    "-L/usr/avr32/lib");
  end Linker;

end Test;
