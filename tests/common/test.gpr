project Test is

  for Main use ("main");

  for Source_Dirs use ("./", "../common");

  package Builder is
     for Default_Switches ("Ada")
       use ("--GCC=avr32-gcc",
            "--RTS=../../build/");
  end Builder;

  package Compiler is
     for Default_Switches ("Ada")
       use ("-gnatn",
--            "-gnatw.X",
--            "-nostartfiles",
            "-O2");
  end Compiler;

  package Linker is
     for Default_Switches ("Ada")
       use ("--GCC=avr32-gcc",
            "--LINK=avr32-gcc",
            "-nostartfiles",
	    "ada_crt0.o",
--  	      "-T../common/link.x",
--            "-D0",
--            "-d1k",
            "-march=avr32elf",
            "-mpart=uc3a0512es");
  end Linker;

end Test;
